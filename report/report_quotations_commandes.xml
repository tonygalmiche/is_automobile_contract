<?xml version="1.0" encoding="utf-8"?><openerp><data>		<!-- Lignes de devis -->    <record id="view_sale_quotations_tree" model="ir.ui.view">        <field name="name">report.sale.quotation.tree</field>        <field name="model">report.sale.quotation</field>        <field name="arch" type="xml">            <tree string="Lignes des devis" create="false">                <field name="partner_id"/>                <field name="name" />                <field name="product_id"/>                <field name="date_order" />				<field name="date_livraison" />				<field name="date_expedition" />				<field name="quantity" />				<field name="type" />                <field name="state" invisible="1"/>           </tree>        </field>    </record>    <record id="view_sale_quotations_graph" model="ir.ui.view">         <field name="name">report.sale.quotation.graph</field>         <field name="model">report.sale.quotation</field>         <field name="arch" type="xml">             <graph string="Lignes des devis" type="bar">                 <field name="product_id"/>                 <field name="quantity" operator="+"/>             </graph>         </field>    </record>    <record id="view_sale_quotations_search" model="ir.ui.view">        <field name="name">report.sale.quotation.search</field>        <field name="model">report.sale.quotation</field>        <field name="arch" type="xml">            <search string="Lignes des devis">                <filter icon="terp-go-home" name="ferme" string="Type Ferme" domain="[('type','=','ferme')]"/>                <filter icon="terp-go-home" name="previsionnel" string="Previsionnel" domain="[('type','=','previsionnel')]"/>                               <field name="partner_id" context="{'contact_display':'partner'}"/>				<field name="name" />				<field name="product_id" default_focus="1"/>                <group expand="1" string="Group By...">                    <filter name="group_partner" string="Client" icon="terp-partner" context="{'group_by':'partner_id'}"/>                    <filter name="group_name" string="Numero devis" icon="terp-gtk-jump-to-rtl" context="{'group_by':'name'}"/>                    <filter name="group_product" string="Produit" icon="terp-accessories-archiver" context="{'group_by':'product_id'}"/>                    <filter name="group_type" string="Type devis" icon="terp-gtk-jump-to-rtl" context="{'group_by':'type'}"/>                </group>            </search>        </field>    </record>    <record id="action_sale_quotations_report" model="ir.actions.act_window">        <field name="name">Lignes des devis</field>        <field name="res_model">report.sale.quotation</field>        <field name="view_type">form</field>        <field name="view_mode">tree,graph</field>		<field name="search_view_id"  eval="False"/>    </record>    <menuitem action="action_sale_quotations_report" id="menu_action_sale_quotations_report" parent="menu_is_ventes" sequence="3"/>    <!-- lignes de commandes -->    <record id="view_sale_commandes_tree" model="ir.ui.view">        <field name="name">report.sale.commande.tree</field>        <field name="model">report.sale.commande</field>        <field name="arch" type="xml">            <tree string="Lignes des commandes" create="false">                <field name="partner_id"/>                <field name="name" />                <field name="product_id"/>                <field name="date_order" />				<field name="date_livraison" />				<field name="date_expedition" />				<field name="quantity" />				<field name="type" />                <field name="state" invisible="1"/>           </tree>        </field>    </record>    <record id="view_sale_commandes_graph" model="ir.ui.view">         <field name="name">report.sale.commande.graph</field>         <field name="model">report.sale.commande</field>         <field name="arch" type="xml">             <graph string="Lignes des devis" type="bar">                 <field name="product_id"/>                 <field name="quantity" operator="+"/>             </graph>         </field>    </record>    <record id="view_sale_commandes_search" model="ir.ui.view">        <field name="name">report.sale.commande.search</field>        <field name="model">report.sale.commande</field>        <field name="arch" type="xml">            <search string="Lignes des commandes">                <filter icon="terp-go-home" name="ferme" string="Type Ferme" domain="[('type','=','ferme')]"/>                <filter icon="terp-go-home" name="previsionnel" string="Previsionnel" domain="[('type','=','previsionnel')]"/>                               <field name="partner_id" context="{'contact_display':'partner'}"/>				<field name="name" />				<field name="product_id" default_focus="1"/>                <group expand="1" string="Group By...">                    <filter name="group_partner" string="Client" icon="terp-partner" context="{'group_by':'partner_id'}"/>                    <filter name="group_name" string="Numero devis" icon="terp-gtk-jump-to-rtl" context="{'group_by':'name'}"/>                    <filter name="group_product" string="Produit" icon="terp-accessories-archiver" context="{'group_by':'product_id'}"/>                    <filter name="group_type" string="Type devis" icon="terp-gtk-jump-to-rtl" context="{'group_by':'type'}"/>                </group>            </search>        </field>    </record>    <record id="action_sale_commandes_report" model="ir.actions.act_window">        <field name="name">Lignes des commandes</field>        <field name="res_model">report.sale.commande</field>        <field name="view_type">form</field>        <field name="view_mode">tree,graph</field>		<field name="search_view_id"  eval="False"/>    </record>    <menuitem action="action_sale_commandes_report" id="menu_action_sale_commandes_report" parent="menu_is_ventes" sequence="4"/></data></openerp>